help: ARP cache entry timeout in seconds

type: u32

create:expression: "sudo sh -c \"echo $((1000*$VAR(@))) > /proc/sys/net/ipv4/neigh//$VAR(../../../@)\.$VAR(../../@)/base_reachable_time_ms\" "

